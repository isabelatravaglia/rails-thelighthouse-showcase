<div class="subbanner--white">
  <div class="subbanner--white__title-box">
    <h2 class="subbanner--white__title">Serviços</h2>
    <hr class="subbanner--white__hr">
    <%# <h3 class="subbanner--white__title">Para Você e para sua Empresa</h3> %>
  </div>
  <div class="subbanner--white__title-box">
    <%# <h2 class="subbanner--white__subtitle">Para Você e Para Sua Empresa</h2> %>
    <%# <hr class="subbanner--white__hr"> %>
    <%# <h3 class="subbanner--white__title">Para Você e para sua Empresa</h3> %>
  </div>
  <div class="service-card-grid">
    <% @services.order(:id).each do |service| %>
      <%= link_to "services" do %>
        <div class="grid-service-card" style="background-image: url(<%= asset_path service.photo.filename %>)">
          <div class="grid-service-card__text-box">
            <div class="grid-service-card__text-box__h1"><%= service.nickname %></div>
            <div class="grid-service-card__text-box__h2"><%= service.tagline %></div>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
